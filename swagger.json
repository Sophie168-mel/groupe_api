          "longitude": {
            "type": "number",
            "example": 2347.8747
          },
          "commentaire": {
            "type": "string",
            "example": "ajout POI"
          }
        },
        "xml": {
          "name": "Point"
        }
      },
      "Limite": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "geohash": {
            "type": "string",
            "example": "u0hnk75h44"
